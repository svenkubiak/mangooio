						<source>${java.version}</source>
						<target>${java.version}</target>
						<compilerArgument>-parameters</compilerArgument>
						<optimize>true</optimize>
						<debug>false</debug>
						<showDeprecation>true</showDeprecation>
						<fork>true</fork>
					</configuration>
				</plugin>
				<plugin>
